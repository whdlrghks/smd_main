  var SSG_check = <%=SSG_reserved%>;
  var body = {
    'user_id' : <%=user_id%>
  }
  $.ajax({
    type: "POST",
    url: "/reserve/refresh",
    async : true,
    data : body,
    dataType : "JSON",
    success: function(json) {
      console.log("refresh");

